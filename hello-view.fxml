<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.automotivedatabase.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <ComboBox fx:id="yearComboBox" prefWidth="150.0" promptText="Select Year" />
    <ComboBox fx:id="makeComboBox" prefWidth="150.0" promptText="Select Make" />
    <ComboBox fx:id="modelComboBox" prefWidth="150.0" promptText="Select Model" />
    <Label fx:id="highwayMpgLabel" />
    <Label fx:id="cityMpgLabel" />
</VBox>
